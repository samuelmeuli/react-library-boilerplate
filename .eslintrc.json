{
	"env": {
		"browser": true
	},
	"extends": "airbnb-base",
	"rules": {
		"brace-style": [2, "stroustrup"],
		"class-methods-use-this": 0,
		"comma-dangle": [2, "never"],
		"eol-last": [2, "never"],
		"indent": [2, "tab", { "SwitchCase": 1 }],
		"no-console": 0,
		"no-else-return": 0,
		"no-tabs": 0,
		"object-curly-newline": 0,
		"object-curly-spacing": [2, "always", { "objectsInObjects": false }],
		"padded-blocks": 0
	}
}